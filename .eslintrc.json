{
    "env": {
      "browser": true,
      "es2021": true
    },
    "parser": "babel-eslint",
    "extends": ["plugin:react/recommended", "airbnb"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/prop-types": 0,
      "react/jsx-props-no-spreading": 0,
      "import/no-extraneous-dependencies": 0,
      "react/jsx-wrap-multilines": 0,
      "object-curly-newline": "off",
      "comma-dangle": "off"
    }
  }